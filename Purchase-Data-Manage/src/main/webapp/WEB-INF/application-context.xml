<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
<import resource="classpath:webSecurityConfig.xml" />
<bean id="userBean" class="mdp.users.beans.User">
<property name="username" value="user1"></property>
<property name="password" value="password"></property>
<property name="firstname" value="user1"></property>
<property name="lastname" value="user1"></property>
<property name="email" value="user1@user.user"></property>
<property name="address" value="user1 rue user 1"></property>
</bean>
</beans>
